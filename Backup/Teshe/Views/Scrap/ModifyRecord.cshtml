@model List<Teshe.Models.ScrapModifyRecord>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>修改记录</title>
    @Styles.Render("~/Content/bootstrap.min.css")
    @Scripts.Render("~/Scripts/bootstrap.min.js")
</head>
<body>
    <table class="table">
        <tr>
            <th>修改内容</th>
            <th>修改时间</th>
        </tr>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Content</td>
                <td>@i.ModifyTime</td>
            </tr>
        }
    </table>
</body>
</html>
